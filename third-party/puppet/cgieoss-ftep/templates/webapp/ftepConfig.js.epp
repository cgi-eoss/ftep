<%- |
    String $ftep_url,
    String $api_url,
    String $api_v2_url,
    String $sso_url,
    String $mapbox_token,
| -%>
// Managed by Puppet
define('ftepConfig', [], function () {
  return {
    ftepUrl: "<%= $ftep_url %>",
    apiUrl: "<%= $api_url %>",
    apiUrlv2: "<%= $api_v2_url %>",
    ssoUrl: "<%= $sso_url %>",
    mapboxToken: "<%= $mapbox_token %>"
  }
});
