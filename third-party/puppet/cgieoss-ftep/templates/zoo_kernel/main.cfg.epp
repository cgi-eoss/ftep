<%- |
    String $wps_version,
    String $lang,
    String $server_address,
    String $data_path,
    String $tmp_path,
    String $tmp_url,
    String $cache_dir,

    String $provider_name,
    String $provider_site,

    String $db_name,
    String $db_host,
    String $db_port,
    String $db_type,
    String $db_schema,
    String $db_user,
    String $db_pass,

    Hash $env_config,
    Hash $ftep_config,
    Hash $java_config,
    Hash $other_config,
| -%>
# Managed by Puppet
[headers]
X-Powered-By=F-TEP

[main]
encoding = utf-8
version = <%= $wps_version %>
lang = <%= $lang %>
serverAddress = <%= $server_address %>
dataPath = <%= $data_path %>
tmpPath = <%= $tmp_path %>
tmpUrl = <%= $tmp_url %>
cacheDir = <%= $cache_dir %>

[identification]
title = Forestry TEP (F-TEP)  WPS Server
abstract = WPS services for Forestry TEP
fees = None
accessConstraints = none
keywords = WPS,GIS,FTEP,F-TEP,Forestry

[provider]
providerName = <%= $provider_name %>
providerSite = <%= $provider_site %>

[database]
dbname = <%= $db_name %>
host = <%= $db_host %>
port = <%= $db_port %>
type = <%= $db_type %>
schema = <%= $db_schema %>
user = <%= $db_user %>
password = <%= $db_pass %>

[javax]
ss = 2m

[javaxx]
MaxPermSize = 64m

<% if !$env_config.empty { -%>
[env]
<% $env_config.each |$key, $val| { -%>
<%= $key %> = <%= $val %>
<% } -%>
<% } -%>

<% if !$ftep_config.empty { -%>
[ftep]
<% $ftep_config.each |$key, $val| { -%>
<%= $key %> = <%= $val %>
<% } -%>
<% } -%>

<% if !$java_config.empty { -%>
[java]
<% $java_config.each |$key, $val| { -%>
<%= $key %> = <%= $val %>
<% } -%>
<% } -%>

<% if !$other_config.empty { -%>
<% $other_config.each |$section, $config| { -%>
[<%= $section %>]
<% $config.each |$key, $val| { -%>
<%= $key %> = <%= $val %>
<% } -%>
<% } -%>
<% } -%>
